{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": 0,
        "quotes": "off",
        "no-plusplus":"off",
        "prefer-template":"off",
        "no-useless-concat":"off",
        "no-use-before-define":"off"
    }
}
